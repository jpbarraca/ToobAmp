<http://two-play.com/rerdavies#me>
	a foaf:Person ;
	foaf:name "Robin Davies" ;
	foaf:mbox <mailto:rerdavies@gmail.com> ;
	foaf:homepage <https://github.com/sponsors/rerdavies> .

<http://two-play.com/plugins/toob-graphiceq>
        a lv2:Plugin ,
                lv2:UtilityPlugin ;
        doap:name "TooB Graphic Eq" ,
                "TooB GraphicEq"@en-gb 
                ;
        uiext:ui <http://two-play.com/plugins/toob-graphiceq-ui> ;
        doap:license <https://rerdavies.github.io/pipedal/LicenseToobAmp> ;
        doap:maintainer <http://two-play.com/rerdavies#me> ;
        lv2:minorVersion 0 ;
        lv2:microVersion ${CMAKE_PROJECT_VERSION_PATCH} ;
        rdfs:comment """
7-band graphic eq. 

""" ;

        mod:brand "TooB";
        mod:label "Graphic Eq";
        lv2:optionalFeature lv2:hardRTCapable;

    lv2:port [
        a lv2:AudioPort ,
          lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in_left" ;
        lv2:name "In L"
    ] , [
        a lv2:AudioPort ,
          lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in_right" ;
        lv2:name "In R"
    ] , [
        a lv2:AudioPort ,
          lv2:OutputPort ;
        lv2:index 2 ;
        lv2:symbol "out_left" ;
        lv2:name "Out L"
    ] , [
        a lv2:AudioPort ,
          lv2:OutputPort ;
        lv2:index 3 ;
        lv2:symbol "out_right" ;
        lv2:name "Out R"
    ] , [ 
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 4 ;
        lv2:symbol "gain_100hz" ;
        lv2:name "100" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ] , [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "gain_200hz" ;
        lv2:name "200" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ] , [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "gain_400hz" ;
        lv2:name "400" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ] , [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 7 ;
        lv2:symbol "gain_800hz" ;
        lv2:name "800" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ] , [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 8 ;
        lv2:symbol "gain_1600hz" ;
        lv2:name "1.6k" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ] , [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 9 ;
        lv2:symbol "gain_3200hz" ;
        lv2:name "3.2k" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ] , [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 10 ;
        lv2:symbol "gain_6400hz" ;
        lv2:name "6.4k" ;
        lv2:default 0.0 ;
        lv2:minimum -15.0 ;
        lv2:maximum 15.0 ;
        units:unit units:db ;
    ], [
        a lv2:ControlPort ,
          lv2:InputPort ;
        lv2:index 11 ;
        lv2:symbol "level" ;
        lv2:name "Level" ;
        lv2:default 0.0 ;
        lv2:minimum -30.0 ;
        lv2:maximum 30.0 ;
        units:unit units:db ;
    ]         .

<http://two-play.com/plugins/toob-graphiceq-ui> 
    a  uiext:X11UI ;
    lv2:binary <ToobAmpUI.so>;

    lv2:extensionData uiext::idle ;
    lv2:extensionData uiext:resize ;
    lv2:extensionData uiext:idleInterface;
    lv2:requiredFeature uiext:idleInterface ;
    .



